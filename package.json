{
  "name": "automacao_cypress_amazon",
  "version": "1.0.0",
  "description": "Projeto de automação da amazon",
  "main": "index.js",
  "scripts": {
    "cy:open": "npx cypress open --config watchForFileChanges=false",
    "cy:run": "npx cypress run  && npm run generate:report",
    "generate:report": "node cucumber_html_config.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "cypress": "^12.11.0",
    "cypress-if": "^1.10.5",
    "cypress-multi-reporters": "^1.6.3",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "multiple-cucumber-html-reporter": "^3.5.0"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/steps",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-cucumber-json",
      "filePrefix": "",
      "fileSuffix": "-feature"
    }
  }
}
